{
    "name": "t0mmy742/respect-validation-translation",
    "type": "library",
    "description": "Translation tool for Respect/Validation library",
    "keywords": ["respect", "validation", "translatation", "localization"],
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas LE BERRE",
            "email": "29512640+t0mmy742@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-gettext": "*"
    },
    "require-dev": {
        "nikic/php-parser": "^4.10",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.80",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "respect/validation": "^2.2",
        "squizlabs/php_codesniffer": "^3.5",
        "thecodingmachine/phpstan-strict-rules": "^0.12.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "t0mmy742\\RespectValidationTranslation\\": "src/"
        }
    }
